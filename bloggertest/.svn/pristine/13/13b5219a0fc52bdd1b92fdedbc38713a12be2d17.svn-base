	<body>
		<div style="padding-left: 10%; padding-right: 10%;">
			<div>
				<ol class="breadcrumb mt20 mb20 ">
					<li>
						<a href="">我的题目</a>
					</li>
					<li class="active">编辑题库</li>

				</ol>
			</div>
			<div class="row">
				<div class="col-xs-12">
					<div class="col-xs-3">
						<div id="paper-form-nav" class="pad paper-form-nav unselectable affix-top" style="position: fixed; margin-top: 1px;">
							<ul class="part nav" style="width: 240px;">
								<li class style="padding-bottom: 5px;">
									<a class="btn" href="#a1">基本信息</a>
								</li>
								<li class style="padding-bottom: 5px;">
									<a class="btn" href="#paper-part-0">单选题</a>
								</li>
								<li class style="padding-bottom: 5px;">
									<a class="btn" href="#paper-part-1">多选题</a>
								</li>
								<li class style="padding-bottom: 5px;">
									<a class="btn" href="#paper-part-2">判断题</a>
								</li>
								<li class style="padding-bottom: 5px;">
									<a class="btn" href="#paper-part-3">填空题</a>
								</li>
								<li class style="padding-bottom: 5px;">
									<a class="btn" href="#paper-part-4">简答题</a>
								</li>
							</ul>
							<div class="part summary" style="padding-top: 10px;">
								<div>
									<table class="w">
										<tbody>
											<tr class="score-wrap">
												<td>目前总题数:</td>
												<td class="tr text-medium-gray"><span class="num text-golden-yellow inline-block mb5 mr5" th:text="${Bank.questionnum}">0</span> 道&emsp;
												</td>
											</tr>
										</tbody>
									</table>
									<table class="w">
										<tbody>
											<tr>
												<td>
													<a class="btn btn-warning btn-block btn-save-bank">完成编辑</a>
												</td>
											</tr>
										</tbody>
									</table>
								</div>
							</div>
						</div>
					</div>
					<!--
          	描述：以上是左侧导航
          -->
					<div class="col-xs-9">
						<div class="paper-form-container">
							<div class="section-basic" id="a1">
								<div>
									<a class="anchor-mark" id="anchor-paper-name"></a> <input type="text" th:value="${Bank.bankname}" class="form-control form-input input-lg paper-name mb10" placeholder="题库名称" disabled/>
								</div>
								<div>
									<textarea class="form-control form-input paper-guide" placeholder="题库描述" th:text="${Bank.bankdescription}" disabled=/></textarea>
								</div>
							</div>
							<!--
                  	描述：以上是考试说明
                  -->
							<!--------------------------------------单选题---------------------------------------->
							<div class="section-part" id="paper-part-0">
								<div class="part-header">
									<form class="form-inline part-name-wrap">
										<div class="form-group" style="width: 100%;">
											<label>单选题</label>
										</div>
									</form>
								</div>
								<fieldset class="mt20" style="padding: 20px 10px;">
									<legend>
										<i class="fa fa-bar-chart"></i> 题目统计
									</legend>
									<label class="part-summary unselectable">
									<div class="part-summary-container" style="display: block;">
										
						<ul class="summary-list" style="padding-left: 10%;">
							<li class="summary-item summary-header"><span> <span
													class="text-ellipsis">知识点 </span>
				</span> <span> <span class="text-ellipsis">题目数 </span>
				</span>  <span> <span class="text-ellipsis">操作 </span>
				</span>
			</li>
			<li class="summary-item" th:each="konw:${kownledgsinlefor}">
				            	<span><span class="skill-item text-ellipsis" th:text="${konw.name}"></span></span>
					            <span><span class="count-item text-highlight" th:text="${konw.num}">1</span></span>
					            <span class="action-item"><a class="btn-remove"><i class="fa fa-trash"></i></a></span>
				        	</li>
						</ul>
					</div>
					</label>
					</fieldset>
					<fieldset class="mt20" style="padding: 20px 10px;">
						<legend>
							<i class="fa fa-bar-chart"></i> 题目详情 <label class="hide-input unselectable q-reorder-toggle"><span>收起</span>
										<i class="fa fa-angle-double-up"></i> </label>
						</legend>
						<div class="part-questions-container">
							<div class="pin-wrapper" style="height: 52px;">
								<div class="part-questions-toolbar unselectable" style="width: 760px;">
									<ul class="list-inline inline-block" style="display: inline-block;">
										<li style="padding-top: 5px; padding-bottom: 6px;">

										<div class="checkall checkbox checkbox-success checkbox-inline">
											<input type="checkbox" value="option1"> <label for="inlineCheckbox2">
											全选 </label>
										</div>
										</li>
										<li>
											<a class="btn btn-default btn-sm delete-sure-btn" data-action="remove"> <i class="fa fa-trash"></i> 删除
											</a>
										</li>
										<li>
                                            <div class="btn-group">
							                    <a class="btn btn-default btn-sm btn-up" data-action="up">
							                        <i class="fa fa-arrow-up"></i> 上移
							                    </a>
							                    <a class="btn btn-default btn-sm btn-down" data-action="down">
							                        <i class="fa fa-arrow-down"></i> 下移
							                    </a>
							                </div>
							            </li>
									</ul>
									<ul class="list-inline f12 pull-right">
										<li>总题数： <span class="text-golden-yellow"></span> <span class="text-medium-gray ml5 mr15" th:text="${Bank.singlenum}">道</span>
										</li>
										<li>
											<a class="btn btn-success btn-sm btn-add-singlequestions" style="display: inline-block;"> <i class="fa fa-plus"></i> 添加题目
											</a>
										</li>
									</ul>
									<div class="cb"></div>
								</div>
							</div>

							<!-- 单选题样式 -->
							<ul class="q-list" id="singlediv">
								<li th:each="single :${singles}" class="q-item sets-question-item" th:data-qid="${single.qId}">
									<div class="check-wrap">
													<div class="checkbox checkbox-success checkbox-inline">
											<input type="checkbox" th:data-qid="${single.qId}" value="option1"/> <label>
											 </label>
										</div>
									</div>
									<div class="q-data">
										<div class="select-toolbar-wrap">
        
        <a class="btn-remove delete-single"><i class="fa fa-trash"></i></a>
        <a class="btn-edit edit-single"><i class="fa fa-edit"></i></a>
    </div>
										<span class="q-sequence" th:text="${singleStat.count}">1</span>
										<span class="text-medium-gray">[单选题]</span>
										<span class="ml5 label-skill text-ellipsis" th:text="${single.qKnowledge}"></span>
										<span class="ml5 label-difficulty text-ellipsis" th:if="${single.qDifficulty==0}">易</span>
										<span class="ml5 label-difficulty text-ellipsis" th:if="${single.qDifficulty==1}">中</span>
										<span class="ml5 label-difficulty text-ellipsis" th:if="${single.qDifficulty==2}">难</span>
										<label class="hide-input unselectable q-expand-toggle independent" style="margin-left: 0"> 
												<a>收起</a> 
												<i class="fa fa-angle-double-up "></i>
												</label>

										<div class="q-title" style="margin-top: 10px;">
											<pre th:text="${single.qTitle}"></pre>

										</div>
										<ul class="list-title-media mt10"></ul>

										<div>

											<ul class="q-options-wrap mt20">
												<li class="q-option" th:each="singlechoice :${single.singlelist}">
													<label class="q-choice-icon radio-inline sets-radio active" th:if="${singlechoice.type==1}">
													<span class="fa-stack radio-icon"> 
														<i class="fa fa-circle-thin fa-stack-2x"></i> 
														<i class="fa fa-dot-circle-o fa-stack-1x select" style="opacity: 1;"></i>
														
													</span>
														</label> 
														
														<label class="q-choice-icon radio-inline sets-radio" th:if="${singlechoice.type!=1}"> 
															<span class="fa-stack radio-icon"> 
														<i class="fa fa-circle-thin fa-stack-2x"></i> 
														<i class="fa fa-dot-circle-o fa-stack-1x select"></i>
													</span>
															
														</label>
														<span class="option-index" th:text="A、" th:if="${singlechoiceStat.count==1}"></span> 
														<span class="option-index" th:text="B、" th:if="${singlechoiceStat.count==2}"></span> <span class="option-index" th:text="C、" th:if="${singlechoiceStat.count==3}"></span> <span class="option-index" th:text="D、" th:if="${singlechoiceStat.count==4}"></span> <span class="option-index" th:text="F、" th:if="${singlechoiceStat.count==5}"></span> <span class="option-index" th:text="G、" th:if="${singlechoiceStat.count==6}"></span> <span class="option-index" th:text="H、" th:if="${singlechoiceStat.count==7}"></span> <span class="option-index" th:text="I、" th:if="${singlechoiceStat.count==8}"></span> <span class="option-index" th:text="J、" th:if="${singlechoiceStat.count==9}"></span> <span class="option-index" th:text="K、" th:if="${singlechoiceStat.count==10}"></span> <span class="option-index" th:text="L、" th:if="${singlechoiceStat.count==11}"></span> <span class="option-index" th:text="M、" th:if="${singlechoiceStat.count==12}"></span>
													<div class="q-option-content pl5">
														<pre th:text="${singlechoice.content}"></pre>
													</div>
												</li>
											</ul>
											<div class="q-answer mt20"></div>
											<div class="q-answer mt20" style="display: block;">
												<p class="q-ans-label">题目解析：</p>
												<span th:if="${single.qAnalysis!=null}" th:text="${single.qAnalysis}"></span>
												<span th:if="${single.qAnalysis==null}" th:text="无"></span>
											</div>
										</div>
									</div>
								</li>
							</ul>
						</div>
						<!--    <ul class="part-question-list q-list" data-link="visible{:_toggleContent}" data-jsv-df="" style="display: block;"><p data-jsv="#144^#44_/44_#45_">该部分下还没有试题，请从这里添加 <i class="fa fa-hand-o-up"></i></p></ul>-->
						<!--选择添加题目时展开-->
					</fieldset>

				</div>

				<!-------------------------------- 多选题 ---------------------------- -->

				<div class="section-part" id="paper-part-1">
					<div class="part-header">
						<form class="form-inline part-name-wrap">
							<div class="form-group" style="width: 100%;">
								<label>多选题</label>
							</div>
						</form>
					</div>
					<fieldset class="mt20">
						<legend>
							<i class="fa fa-bar-chart"></i> 题目统计
						</legend>
						<label class="part-summary unselectable">
									<div class="part-summary-container" style="display: block;">
										
			<ul class="summary-list" style="padding-left: 10%;">
				<li class="summary-item summary-header"><span> <span
													class="text-ellipsis">知识点 </span>
								</span> <span> <span class="text-ellipsis">题目数 </span>
								</span>  <span> <span class="text-ellipsis">操作 </span>
								</span>
							</li>
							<li class="summary-item" th:each="konw:${kownledgsinmufor}">
				            	<span><span class="skill-item text-ellipsis" th:text="${konw.name}"></span></span>
					            <span><span class="count-item text-highlight" th:text="${konw.num}">1</span></span>
					            <span class="action-item"><a class="btn-remove"><i class="fa fa-trash"></i></a></span>
				        	</li>
			</ul>
		</div>
		</label>
		</fieldset>
		<fieldset class="mt20" style="padding: 20px 10px;">
			<legend>
				<i class="fa fa-bar-chart"></i> 题目详情 <label class="hide-input unselectable q-reorder-toggle"><span>收起</span>
										<i class="fa fa-angle-double-up"></i> </label>
			</legend>
			<div class="part-questions-container">
				<div class="pin-wrapper" style="height: 52px;">
					<div class="part-questions-toolbar unselectable" style="width: 760px;">
						<ul class="list-inline inline-block" style="display: inline-block;">
							<li style="padding-top: 5px; padding-bottom: 6px;">
<div class="checkall checkbox checkbox-success checkbox-inline">
											<input type="checkbox" value="option1"> <label for="inlineCheckbox2">
											全选 </label>
										</div>
							</li>
							
							<li>
								<a class="btn btn-default btn-sm delete-sure-btn" data-action="remove"> <i class="fa fa-trash"></i> 删除
								</a>
							</li>
							<li>
                                            <div class="btn-group">
							                    <a class="btn btn-default btn-sm btn-up" data-action="up">
							                        <i class="fa fa-arrow-up"></i> 上移
							                    </a>
							                    <a class="btn btn-default btn-sm btn-down" data-action="down">
							                        <i class="fa fa-arrow-down"></i> 下移
							                    </a>
							                </div>
							            </li>
						</ul>
						<ul class="list-inline f12 pull-right">
							<li>总题数： <span class="text-golden-yellow" th:text="${Bank.multiplenum}"></span> <span class="text-medium-gray ml5 mr15" >道</span>
							</li>
							<li>
								<a class="btn btn-success btn-sm btn-add-multiplequestions" style="display: inline-block;"> <i class="fa fa-plus"></i> 添加题目
								</a>
							</li>
						</ul>
						<div class="cb"></div>
					</div>
				</div>
				<ul class="q-list" id="multiplydiv">
					<li th:each="multiple :${multiples}" class="q-item sets-question-item"  th:data-qid="${multiple.qId}">
						<div class="check-wrap">
								<div class="checkbox checkbox-success checkbox-inline">
											<input type="checkbox" th:data-qid="${multiple.qId}" value="option1"/> <label>
											 </label>
										</div>
						</div>
						<div class="q-data">
<div class="select-toolbar-wrap">
        <a class="btn-remove delete-multiple"><i class="fa fa-trash"></i></a>
        <a class="btn-edit edit-multiple"><i class="fa fa-edit"></i></a>
    </div>
							<span class="q-sequence" th:text="${multipleStat.count}">1</span>
							<span class="text-medium-gray">[多选题 ]</span>
							<span class="ml5 label-skill text-ellipsis" th:text="${multiple.qKnowledge}"></span>
							<span class="ml5 label-difficulty text-ellipsis" th:if="${multiple.qDifficulty==0}">易</span>
							<span class="ml5 label-difficulty text-ellipsis" th:if="${multiple.qDifficulty==1}">中</span>
							<span class="ml5 label-difficulty text-ellipsis" th:if="${multiple.qDifficulty==2}">难</span>
							<label class="hide-input unselectable q-expand-toggle independent" style="margin-left: 0">
						<a>收起</a>
						<i  class="fa fa-angle-double-up "></i>
					</label>

							<div class="q-title" style="margin-top: 10px;">
								<pre th:text="${multiple.qTitle}"></pre>
							</div>
							<ul class="list-title-media mt10"></ul>

							<div data-link="">

								<ul class="q-options-wrap mt20" data-jsv-df="">
									<li class="q-option" th:each="multiplechoice :${multiple.multiplelist}">
										<label class="q-choice-icon check-box inline-block active" th:if="${multiplechoice.type==1}">
													<span class="fa-stack check-icon"> 
														<i class="fa fa-square-o fa-stack-2x"></i> 
														<i class="fa fa-check-square fa-stack-1x check" style="opacity: 1;"></i>
														
													</span>
														</label> 
														
														<label class="q-choice-icon check-box inline-block" th:if="${multiplechoice.type!=1}"> 
															<span class="fa-stack check-icon"> 
														<i class="fa fa-square-o fa-stack-2x"></i> 
														<i class="fa fa-check-square fa-stack-1x check"></i>
													</span>
															
														</label>
										<span class="option-index" th:text="A、" th:if="${multiplechoiceStat.count==1}"></span> 
										<span class="option-index" th:text="B、" th:if="${multiplechoiceStat.count==2}"></span> <span class="option-index" th:text="C、" th:if="${multiplechoiceStat.count==3}"></span> <span class="option-index" th:text="D、" th:if="${multiplechoiceStat.count==4}"></span> <span class="option-index" th:text="F、" th:if="${multiplechoiceStat.count==5}"></span> <span class="option-index" th:text="G、" th:if="${multiplechoiceStat.count==6}"></span> <span class="option-index" th:text="H、" th:if="${multiplechoiceStat.count==7}"></span> <span class="option-index" th:text="I、" th:if="${multiplechoiceStat.count==8}"></span> <span class="option-index" th:text="J、" th:if="${multiplechoiceStat.count==9}"></span> <span class="option-index" th:text="K、" th:if="${multiplechoiceStat.count==10}"></span> <span class="option-index" th:text="L、" th:if="${multiplechoiceStat.count==11}"></span> <span class="option-index" th:text="M、" th:if="${multiplechoiceStat.count==12}"></span>
										<div class="q-option-content pl5">
											<pre th:text="${multiplechoice.content}"></pre>
										</div>
									</li>
								</ul>
								<div class="q-answer mt20">

								</div>
								<div class="q-answer mt20" style="display: block;">
									<p class="q-ans-label">题目解析：</p>
									<span th:if="${multiple.qAnalysis!=null}" th:text="${multiple.qAnalysis}"></span>
									<span th:if="${multiple.qAnalysis==null}" th:text="无"></span>
								</div>

							</div>
						</div>
					</li>
				</ul>
			</div>
			<!--    <ul class="part-question-list q-list" data-link="visible{:_toggleContent}" data-jsv-df="" style="display: block;"><p data-jsv="#144^#44_/44_#45_">该部分下还没有试题，请从这里添加 <i class="fa fa-hand-o-up"></i></p></ul>-->
			<!--选择添加题目时展开-->
		</fieldset>

		</div>

		<!-------------------------------- 判断题 ---------------------------- -->

		<div class="section-part" id="paper-part-2">
			<div class="part-header">
				<form class="form-inline part-name-wrap">
					<div class="form-group" style="width: 100%;">
						<label>判断题</label>
					</div>
				</form>
			</div>
			<fieldset class="mt20">
				<legend>
					<i class="fa fa-bar-chart"></i> 题目统计
				</legend>
				<label class="part-summary unselectable">
									<div class="part-summary-container" style="display: block;">
										
		<ul class="summary-list" style="padding-left: 10%;">
			<li class="summary-item summary-header"><span> <span
													class="text-ellipsis">知识点 </span>
								</span> <span> <span class="text-ellipsis">题目数 </span>
								</span> <span> <span class="text-ellipsis">操作 </span>
								</span>
							</li>
			<li class="summary-item" th:each="konw:${kownledgsinjufor}">
				            	<span><span class="skill-item text-ellipsis" th:text="${konw.name}"></span></span>
					            <span><span class="count-item text-highlight" th:text="${konw.num}">1</span></span>
					            <span class="action-item"><a class="btn-remove"><i class="fa fa-trash"></i></a></span>
