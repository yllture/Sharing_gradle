		<script src="../../js/modernizr.min.js" th:src="@{static/js/modernizr.min.js}"></script>
		<script src="../../js/jquery-2.0.3.min.js" type="text/javascript" th:src="@{static/js/jquery-2.0.3.min.js}"></script>
		<script src="../../js/template.js" th:src="@{static/js/template.js}"></script>
