        <script>
        function down(obj){
    		
    		var downpublishbankid=obj;
    	    
    		 $.ajax({
    				
    			    type : 'post',
    				url : 'bankdownpublish',
    				async:false,
    				data:{"bankdownpublish":downpublishbankid},
    				dataType : 'text',
    				success : function(res) {
//     					
    					location.reload();
    			    }
    			 
    			 
    		 })
    		
    	}
    	
        </script>
          <script>
        function downpublish(obj){
        	$("#dialogConfirm").attr("value",obj);
        	$("#dialog").changeClass("modal-block");
        	
        }
        
        
        </script>
