body {
    font-family: "微软雅黑", "Open Sans", Arial, "Hiragino Sans GB", "Microsoft YaHei", "STHeiti", "WenQuanYi Micro Hei", SimSun, sans-serif;
	background-color: #f5f7fa;
}
blockquote, body, button, code, dd, div, dl, dt, fieldset, form, h1, h2, h3, h4, h5, h6, input, legend, li, ol, p, pre, td, textarea, th, ul {
    margin: 0;
    padding: 0;
}
a:hover{
	text-decoration:none;
	cursor: pointer;
}
a:active{
	text-decoration:none;
	cursor: pointer;
}
a:visited{
	text-decoration:none;
	cursor: pointer;
}
a:link{
	text-decoration:none;
	cursor: pointer;
}
.q-item>div.q-data {
    position: relative;
    float: left;
    width: 100%;
    margin-bottom: 0;
    padding: 10px 25px 10px 15px;
}
.q-item .q-sequence {
    color: green;
    font-size: 1em;
    font-weight: bold;
}
.text-medium-gray {
    color: #AAB2BD;
}
.q-item .label-skill {
    background-color: #E0E0E0;
    color: #FFFFFF;
    padding: 1px 9px;
    border-radius: 4px;
}
.text-ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.ml5 {
    margin-left: 5px;
}
.q-item .label-difficulty {
    background-color: orange;
    color: #FFFFFF;
    padding: 1px 9px;
    border-radius: 4px;
}
.q-list .q-item {
    width: 100%;
    color: #434A54;
    position: relative;
    display: table;
    font-size: 13px;
    padding: 10px 0;
    border-bottom: 1px dashed #CCD1D9;
}
li {
    list-style: none;
}
.q-item>div.check-wrap {
    width: 1%;
    white-space: nowrap;
    vertical-align: top;
    text-align: center;
}
.q-item>div {
    display: table-cell;
}
.q-item .pure-checkbox {
    width: 40px;
    height: 100%;
    z-index: 100;
    margin: 0;
    cursor: pointer;
}
.pure-checkbox {
    margin-bottom: 0;
    font-weight: normal;
    color: #434A54;
    /* cursor: pointer; */
}
.unselectable {
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}
label {
    display: inline-block;
    max-width: 100%;
    margin-bottom: 5px;
    font-weight: 700;
}
.q-item .pure-checkbox>div {
    border: 1px solid #AFC6DA;
    color: #FFF;
    margin: 12px auto;
}
.pure-checkbox.checkbox-list > div {
    border-radius: 4px;
    width: 16px;
    height: 16px;
    text-align: center;
    line-height: 14px;
}
.pure-checkbox > div {
    border: 1px solid #78a206;
    background-color: #FFF;
    border-radius: 2px;
    width: 20px;
    height: 20px;
    text-align: center;
    line-height: 18px;
    color: #FFF;
    display: inline-block;
}
.pure-checkbox > div > i {
    filter: alpha(opacity=0);
    opacity: 0;
    color: #333333;
}
.pure-checkbox input[type="checkbox"] {
    display: none;
    position: absolute;
    z-index: -1000;
    filter: alpha(opacity=0);
    opacity: 0;
    clip: rect(0, 0, 0, 0);
}
input[type=checkbox], input[type=radio] {
    margin: 4px 0 0;
    line-height: normal;
}
input[type=checkbox], input[type=radio] {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    padding: 0;
}
.q-item .q-title {
    font-size: 1.1em;
    word-break: break-all;
}
pre {
    display: block;
    padding: 9.5px;
    margin: 0 0 10px;
    font-size: 13px;
    line-height: 1.42857143;
    color: #333;
    word-break: break-all;
    word-wrap: break-word;
    background-color: #f5f5f5;
    border: 1px solid #ccc;
    border-radius: 4px;
}
.sets-question-item pre {
    display: inline;
    outline: none;
    border: none;
    background: none;
    font-family: inherit;
    font-size: 1em;
    white-space: pre-wrap;
}
.q-list .q-item img {
	width: 90px;
    max-width: 100%;
}

fieldset, img {
    border: 0;
}
img {
    vertical-align: middle;
}
.q-item .list-title-media {
    border-left: 4px solid #8CC152;
}
.mt10 {
    margin-top: 10px;
 }
.q-item .q-option {
    clear: both;
    position: relative;
    border-radius: 4px;
    padding: 6px 12px;
    margin-bottom: 5px;
    border: 1px solid #E6E9ED;
    /*transition: all 0.15s ease-in-out 0s;
    -webkit-transition: all 0.15s ease-in-out 0s;*/
}
.q-item .q-option .q-choice-icon {
    display: table-cell;
    vertical-align: middle;
    opacity: 0.65;
    cursor: default;
}
label.sets-radio {
    font-weight: 400;
    padding: 0;
    color: #656d78;
    border-color: transparent;
    background: 0 0;
    box-shadow: none;
}
.checkbox-inline, .radio-inline {
    display: inline-block;
    padding-left: 20px;
    margin-bottom: 0;
    font-weight: 400;
    vertical-align: middle;
    cursor: pointer;
}
label.sets-radio>.radio-icon {
    font-size: 10px;
    width: 20px;
    text-align: center;
}
.q-item .q-option .q-choice-icon {
    display: table-cell;
    vertical-align: middle;
    opacity: 0.65;
    cursor: default;
}
.q-item .q-option .q-choice-icon {
    display: table-cell;
    vertical-align: middle;
    opacity: 0.65;
    cursor: default;
}
label.sets-radio .select {
    font-size: 2em;
    line-height: 20px;
    display: inline-block;
    height: 20px;
    opacity: 0;
    filter: alpha(opacity=0);
}
.q-item .q-option .option-index {
    display: table-cell;
    padding-left: 10px;
}
.q-item .q-option .q-option-content {
    display: table-cell;
    min-width: 250px;
}
.pl5 {
    padding-left: 5px;
}
.q-item .q-answer {
    clear: both;
    border-left: 4px solid green;
    padding: 0 10px;
    font-size: 1em;
}
.q-item .q-answer .q-ans-label {
    font-size: 0.9em;
    margin-bottom: 10px;
    color: #AAB2BD;
}
label.sets-radio.active>.radio-icon>i, label.sets-radio>.active>.radio-icon>i {
    color: #78a206;
}
.q-title pre{
	display: block;
}
.q-div{
	margin: 20px 200px;
	margin-bottom: 30px;
	background-color: #FFFFFF;
	border-radius: 5px;
	box-shadow: 0 1px 2px rgba(0,0,0,.2);
}
label.check-box, label.check-box>.btn {
    font-weight: 400;
    padding: 0;
    color: #656d78;
    border-color: transparent;
    background: 0 0;
    box-shadow: none;
}

.inline-block {
    display: inline-block;
}
label.check-box, label.check-box>.btn {
    font-weight: 400;
    padding: 0;
    color: #656d78;
    border-color: transparent;
    background: 0 0;
    box-shadow: none;
}

.inline-block {
    display: inline-block;
}
label.check-box.active .check, label.check-box>.active .check {
    opacity: 1;
    filter: alpha(opacity=100);
}
label.check-box .check {
    font-size: 2em;
    line-height: 20px;
    display: inline-block;
    height: 20px;
    opacity: 0;
    filter: alpha(opacity=0);
}
label.check-box .check-icon, label.check-box>.btn .check-icon {
    font-size: 10px;
    width: 20px;
    text-align: center;
}
label.check-box.active>.check-icon>i, label.check-box>.active>.check-icon>i {
    color: #78a206;
}
.q-div .q-list>li:nth-of-type(even) {
    background-color: #f8fcff;
}
.fill-answer{
	background-color: #78a206;
    padding: 1px 9px;
    color: #FFFFFF;
    border-radius: 4px;
    max-width: 110px;
    display: inline-block;
    vertical-align: middle;
    margin: 2px;
}
.q-item .q-fill-answer{
	border-left: 0px solid green;
	margin-left: 4px;
	margin-bottom: 4px;
}
.q-item .q-fill-answer p{
	display: inline;
}
.detail-infor-div{
	padding: 20px;
	margin:20px 200px;
	
	background-color: #FFFFFF;
}
.q-part{
	margin: 10px;
}
.part-name{
	background-color: #78a206;
	margin: 5px;
