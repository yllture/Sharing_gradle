                 dataView : {show: true, readOnly: false},
                 magicType : {show: true, type: ['line', 'bar']},
                 restore : {show: true},
                 saveAsImage : {show: true}
                 }
              },
             calculable : true,
             xAxis : [
            {
               type : 'category',
               data :  arrTitle/* ['1题','2题','3题','4题','5题','6题','7题','8题','9题','10题','11题','12题'] */
            }
            ],
    yAxis : [
        {
            type : 'value'
        }
    ],
    series : [
       
        {
            name:'正确率',
            type:'bar',
            data:/* [0.5, 0.59, 0.9, 0.264, 0.287, 0.707, 0.1756, 0.1822, 0.487, 0.188, 0.60, 0.23] */arrData,
           /*  markPoint : {
                data : [
                    {name : '正确率最高题目', value : 0.9, xAxis: 2, yAxis: 0.9, symbolSize:18},
                    {name : '正确率最低题目', value : 0.18, xAxis: 9, yAxis: 0.18}
                ]
            }, */
            markLine : {
                data : [
                    {type : 'average', name : '平均值'}
                ]
            }
        }
    ]
};
                    
                // 为echarts对象加载数据 
                myChart.setOption(option); 
            }
            

           	
          
            
           
    );
    </script>
    
    <script type="text/javascript" th:inline="javascript">
    var getdatas=[[${datas}]];
    var obj=JSON.parse(getdatas);
    var scores=obj.scores;
    var scoreLength;
    if(scores==null){
    	scoreLength=0;
    	
    }else{
    	scoreLength=scores.length;
    }
     // 路径配置
     require.config({
            paths: {
                echarts: 'http://echarts.baidu.com/build/dist'
            }
        });
        
        // 使用
        require(
            [
                'echarts',
                'echarts/chart/bar',// 使用柱状图就加载bar模块，按需加载
                'echarts/chart/line'
            ],
            //题目正确率统计
            function (questionScore) {
                // 基于准备好的dom，初始化echarts图表
                 var myChart = questionScore.init(document.getElementById('main1')); 
                 option = {
                 	 title : {
                 text: '某试卷各分数学生人数统计',
                },
    tooltip : {
        trigger: 'axis'
    },
    toolbox: {
        show : true,
        feature : {
            mark : {show: true},
            dataZoom : {show: true},
            dataView : {show: true},
            magicType : {show: true, type: ['line', 'bar']},
            restore : {show: true},
            saveAsImage : {show: true}
        }
    },
    dataZoom : {
        show : true,
        realtime : true,
        y: 36,
        height: 20,
        start : 40,
        end : 60
    },
    xAxis : [
        {
            type : 'category',
            boundaryGap : false,
            data : function (){
                var list = [];
                var n = 0;
                while (n++ < 150) {
                    list.push(n);
                }
                return list;
            }()
        }
    ],
    yAxis : [
        {
            type : 'value'
        }
    ],
    series : [
        {
            name:'dz',
            type:'line',
            data:function (){
                var list = [];
               /*  for (var i = 1; i <= 150; i++) {
                    list.push(Math.round(Math.random()* 30));
                } */
                for(var j=0;j<scoreLength;j++){
                	list[j]=scores[j];
                }
                return list;
            }()
        }
    ],
    calculable:false
};
                    
                    
                // 为echarts对象加载数据 
                myChart.setOption(option); 
            }
            

           	
          
            
           
        );
    	
    	
    	
    </script>
    