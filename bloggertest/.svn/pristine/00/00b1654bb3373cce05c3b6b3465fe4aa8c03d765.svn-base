	     <script type="text/javascript" th:inline="javascript">
	     //获得数据
	     var getdatas=[[${datas}]];
	     var obj=JSON.parse(getdatas);
	     var singleData=obj.singleData;
	     var multipleData=obj.multipleData;
	     var judgeData=obj.judgeData;
	     var fillData=obj.fillData;
	     var shortanswerData=obj.shortanswerData;
	     var arrTitle=[];
	     var arrData=[];
	     var i=0;
	     var singleLength;
	     var multipleLength;
	     var judgeLength;
	     var fillLength;
	     var shortanswerLength;
	  
	     
	     if(singleData!=null){
	    	 
	    	 singleLength=singleData.length;
	     }
	     else{
	    	 singleLength=0;
	     }
	     
        if(multipleData!=null){
	    	 
        	multipleLength=multipleData.length;
	     }
	     else{
	    	 multipleLength=0;
	     }
        
       if(judgeData!=null){
    	   judgeLength=judgeData.length;
    	   }
       else{
    	   judgeLength=0;
    	   }
       
      if(fillData!=null){
    	  fillLength=fillData.length;
    	  }
      else{
	    fillLength=0;
	   }
      
      if(shortanswerData!=null){
    	  shortanswerLength=shortanswerData.length;
    	  }
      else{
    	  shortanswerLength=0;
      }
      alert("singleLength:"+singleLength);
      alert("multipleLength:"+multipleLength);
      alert("judgeLength:"+judgeLength);
      alert("fillLength:"+fillLength);
      alert("shortanswerLength:"+shortanswerLength);
	     
	     for(;i<singleLength;i++){
	    	 
	    	 arrTitle[i]="单选题第"+(i+1)+"题";
	    	 arrData[i]=singleData[i];
	     }
	     
	     i=singleLength;
	     for(;i<singleLength+multipleLength;i++){
	    	 arrTitle[i]="多选题第"+(i+1-singleLength)+"题";
	    	 arrData[i]=multipleData[i-singleLength];
	     }
	     i=multipleLength+singleLength;
	     for(;i<singleLength+multipleLength+judgeLength;i++){
	    	 arrTitle[i]="判断题第"+(i+1-singleLength-multipleLength)+"题";
	    	 arrData[i]=judgeData[i-singleLength-multipleLength];
	     }
	     i=multipleLength+singleLength+judgeLength;
	     for(;i<singleLength+multipleLength+judgeLength+fillLength;i++){
	    	 arrTitle[i]="填空题第"+(i+1-singleLength-multipleLength-judgeLength)+"题";
	    	 arrData[i]=fillData[i-singleLength-multipleLength-fillLength];
	     }
	     i=fillLength+multipleLength+singleLength+judgeLength;
	     for(;i<singleLength+multipleLength+judgeLength+fillLength+shortanswerLength;i++){
	    	 arrTitle[i]="简答题第"+(i+1-singleLength-multipleLength-fillLength-judgeLength)+"题";
	    	 arrData[i]=shortanswerData[i-singleLength-multipleLength-fillLength-judgeLength];
	     }
	     
	     
	     
       
      <!-- ECharts单文件引入 -->
   
