.pad {
    padding: 0;
    border-radius: 0;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.2);
}
.pad > .pad-heading {
    padding: 20px 30px 0;
}
.text-mint {
    color: #37BC9B;
}
.pad > .pad-body {
    padding: 20px 30px;
}
.required:before, .sets-dl dt.required:before {
    font-family: 'FontAwesome';
    content: "\f069";
    color: #E9573F;
    font-size: 8px;
    display: inline-block;
    margin-top: -5px;
    margin-right: 5px;
    vertical-align: middle;
}
.text-highlight {
    color: #F9690E;
    padding: 0 3px;
}
