@charset "UTF-8";

@media (max-width: 1200px){
    .col-lg-2{
        display: none;
    }
}

.content {
    padding-left: 0 !important;
    padding-right: 0 !important;
}

.col-lg-9, .col-lg-2 {
    margin-left: 0 !important;
    margin-right: 0 !important;
}


.col-lg-2{
    padding-right: 0 !important;
    padding-left: 0 !important;
    width: 340px !important;
    transition: all 1s ease 0s;
    position: relative;
	float: right;
	right: 8%;
	width: 24%;
	left: 6%;
}

.user-center{
    width: 100%;
    margin-bottom: 34px;
    padding: 10px;
    background: #fff;
    padding-left: 6%;
    box-sizing: border-box;
    border-radius: 4px;
    font-size: 14px;    
	background-color: #FFFFFF;	
	
	align-content: center;
	text-align: center;
	padding-top: 10%;
	padding-bottom: 10%;
}
.user-center a:hover{
    text-decoration: none;
}

.user-infos a{
    padding: 4px 10px;
    color: #999;
}

.right-list{
    width: 100%;
}

.user-data-single a{
	
    position: relative;
    text-align: center;	
    display: inline-block;
    text-decoration: none;
    
}

.user-data-single .not-last:after{
    content: '';
    position: absolute;
    right: 0;
    top: 50%;
    width: 1px;
    height: 28px;
    background-color: #e9e8ee;
    overflow: hidden;
    margin-top: -14px;
}

.the-num{
	font-weight: 700;
	color: #333333;
}

.headimg{
    margin-top: 10px;
	margin-bottom: 0px;
	width: 100px;
	height: 100px;
	border-radius: 100%;
    border: 2px solid #fff;
    overflow: hidden;
}

.headimg img{
    width: 100%;
}
.headimg a{
	width: 100px;
	height: 100px;
	border-radius: 100%;
   
    overflow: hidden;
}
.headimg:hover{
    border-color: #60BDE8;
}
.answer-item-content img{
	width: 400px;
	height: auto;
}
.list-group-item {
    width: 100%;   
}

.user-info{
    text-align: center;
}

.user-name h4{
    font-size: 15px;
    margin-bottom: 5px;
}

body{
