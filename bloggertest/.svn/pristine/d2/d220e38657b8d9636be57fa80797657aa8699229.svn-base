.adg div,.adg a,.adg i,.adg h3{
	display: inline-block;
}
.adg{
	background-color: #FFFFFF;
}
.adg-right{
	left: 48%;
	position: relative;
	padding-top: 20px;
	
}
.adg-right a{

	padding-left: 20px;
}

.work-bar{
	position: relative; 
	margin-bottom: 10px; 
	background-color: #FFFFFF;
}
.work-bar img{
	width: 45px !important;
    height: 45px !important;
    border-radius: 50%;
    margin-right: 4px;
    margin-bottom: 4px;
    border: 2px solid #37bc9b;
    margin-top:-24px;
    margin-right:20px;
    
}

.creator-img{
	display: inline-block;
}
.paper-matrix{
	display: inline-block;
}

.layui-card img{
	width: 30px;
    height: 30px;
    border-radius: 50%;
    margin-right: 4px;
    margin-bottom: 14px;
}
.clazz-members-wrap img{
    background-color: #fbfbfb;
    display: flex;
    flex-direction: column;
    display: inline-block;
}
.clazz-members-wrap .a,.clazz-members-wrap .span{
	display: inline-block;
	font-size: 20px;
}
.clazz-members-wrap li{
	margin: 10px;
}
.clazz-members-wrap li span{
	font-size: 16px;
	margin: 4px;
}
.gmemdelete{
	position: absolute;
	right: 30px;
	width: 60px;
	background-color: #b0dfb3;
	color: #FFFFFF;
	border-radius: 2px;
	border: 1px solid transparent;
	font-size: 15px;
	margin-top:4px;
	text-align:center;
}
.custom-search{
	padding-top: 10px;
	padding-bottom: 10px;
    position: relative;
    color: #434a54;
    border-width: 2px;
    /*margin-top: 10px;*/
    background-color: #FFFFFF;
    border: 1px solid transparent;
    border-radius: 2px;
    opacity: 0.5;
}
.custom-search input{
	border: 1px solid transparent;
    border-radius: 2px;
    /*padding: 0 28px;*/
    vertical-align: middle;
    font-size: 18px;
}
.custom-search i{
	font-size: 18px;
	padding-left:18px;
}
.paper-matrix{
	width:90%;
}
.mem-paper-action{
	float:right;
}
.paper-basic{
	float:left;
	padding-top:20px;
}
.mem-paper-action a{
	float:left;
	padding-right:40px;
	padding-top:30%;
	font-size:18px;
}
.paper-action a p{
	text-align:center;
}
.creator-img{
	padding-right:18px;
	margin-bottom:6px;
}
.adg{
	position: relative;
    top: 20px;
}
.layui-row{
	position: relative;
    top: 16px;
}
.adg-right{
	display:inline-block;
	width:50%;
	left:48%;
	padding-top:0px;
	padding-bottom:0px;
}
