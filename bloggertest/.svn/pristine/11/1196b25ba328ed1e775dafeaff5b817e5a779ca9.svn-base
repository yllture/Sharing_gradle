		<!-- 商店开始 -->
		<div class="shop-area pt-80 bg-gray">
			<div class="container">
				<div class="row">
					<div class="order-xl-2 order-lg-2 col-xl-9 col-lg-8">
						<div class="ht-product-tab">
							<div class="ht-tab-content">
								<div class="nav" role="tablist">
									<a class="active grid" href="#grid" data-toggle="tab" role="tab" aria-selected="true" aria-controls="grid"><i class="fa fa-th"></i></a>

								</div>
							</div>
							<div class="shop-results-wrapper">
								<div class="shop-results"><span>条数:</span>
									<div class="shop-select"   >
										<select name="number" id="number"  onchange="selectpagesize(this.value)">
											<option value="15" th:text="${session.pagesize}"></option>
											<option value="3" th:if="${session.pagesize} ne '3'">3</option>
											<option value="6" th:if="${session.pagesize} ne '6'">6</option>
											<option value="9" th:if="${session.pagesize} ne '9'">9</option>
											<option value="12" th:if="${session.pagesize} ne '12'">12</option>
											<option value="15" th:if="${session.pagesize} ne '15'">15</option>
										</select>
										<input id="pagesize" type="hidden" th:value="${session.pagesize}" />
									</div>
								</div>
							</div>

						</div>
						<div class="shop-product-tab">
							<div class="res-filter">
								<div class="filter-title">排列方式</div>
								<div class="filter-values">
									<a id="0" onclick="sort(this.id)">最新</a> 
									<a id="1" onclick="sort(this.id)">最热</a>
									
								</div>
							</div> 
							<div class="res-filter">
								<div class="filter-title">收费方式</div>
								<div class="filter-values"> 
									<a href="/tobankshop?chargetype=1">免费</a> 
									<a href="/tobankshop?chargetype=2">收费</a>
								
								</div> 
							</div> 
						</div>
						<div class="ht-product-shop tab-content text-center">
							<div class="tab-pane active show fade" id="grid" role="tabpanel">
								<div class="custom-row"   >
									<div class="custom-col"  th:each="bank:${banks }">
										<div class="single-product-item">
											<div class="product-image">
												<div class="product-hover">
													<div class="product-mask"></div>
													<ul class="hover-icon-list">
														<li>
															<a href="wishlist.html"><i class="icon icon-Heart"></i></a>
														</li>
														<li>
															<a href="" data-toggle="modal" data-target="#productModal"><i class="icon icon-Search"></i></a>
														</li>
													</ul>
													<button type="button" class="p-cart-btn" onclick="buybank(this.id)" th:id=${bank.bankid}>购买试题</button>
												</div>
											</div>
											<div class="product-text">
												<!-- 试卷题目 -->
												<h5><a href="product-details.html"  th:text=${bank.bankname}></a></h5>
												<h6><a th:text=${#dates.format(bank.time,'yyyy-MM-dd-HH：mm')}></a></h6>
												
												<div class="product-number">
													<!-- 题量(题目总数量) -->
													<span class="new-info" title="题量"><h4  th:text=${bank.questionnum}></h4>道题</span>
												</div>

												<div class="pro-price">
													<span class="new-price" ><i class="fa fa-diamond" >积分:</i><i th:text=${bank.price}></i></span>
												</div>
												<div class="pro-info">
													<div class="float-left">
												<!-- 试卷评分星级 -->
												<div class="product-rating">
													<i class="fa fa-star color"></i>
													<i class="fa fa-star color"></i>
													<i class="fa fa-star color"></i>
													<i class="fa fa-star-o"></i>
													<i class="fa fa-star-o"></i>
												</div>
													<!-- 销量 -->
													<a class="new-info" data-toggle="tooltip" title="销量"><i class="fa fa-shopping-cart" th:text=${bank.paynum}></i></a>
													
													</div>
													<div class="float-right">
													<!-- 卖家 -->
													<a class="new-info pro-sold-user" data-toggle="tooltip"  th:title="${bank.user.username}"  ><i class="fa fa-user"  ></i></a>
													</div>
												</div>
											</div>
										</div>
									</div>
									
									
								</div>
							</div>
						</div>
						
						
						
						
						
						
						<div class="pagination-wrapper">
							<p>这里是<span>1</span>-<span>12</span>(共2页)</p>
							<nav aria-label="navigation">

								<ul class="pagination">

									<input class="toPageInput" placeholder="跳转到..." />
									<li class="page-item">
										<a class="page-link" href="#">|
											<</a>
									</li>
									<li class="page-item">
										<a class="page-link" href="#">
											<</a>
									</li>
									<li class="page-item active">
										<a class="page-link" href="#">1</a>
									</li>
									<li class="page-item">
										<a class="page-link" href="#">2</a>
									</li>
									<li class="page-item">
										<a class="page-link" href="#">></a>
									</li>
									<li class="page-item">
										<a class="page-link" href="#">>|</a>
									</li>
								</ul>
							</nav>
						</div>
					</div>
					<div class="col-xl-3 col-lg-4">
						<div class="sidebar-wrapper">
							<h3>IT技术</h3>
							<div class="sidebar-widget">
								<div class="sidebar-widget-option-wrapper">
									<div class="sidebar-widget-option">
										<a for="cat1">软件开发</a>
									</div>
									|
									<div class="sidebar-widget-option">
										<a for="cat2">测试/运维</a>
									</div>
									|
									<div class="sidebar-widget-option">
										<a for="cat3">产品/运营</a>
									</div>
								</div>
							</div>
							<h3>IQ&EQ</h3>
							<div class="sidebar-widget">
								<div class="sidebar-widget-option-wrapper">
									<div class="sidebar-widget-option">
										<a for="black">综合能力</a>
									</div>
									|
									<div class="sidebar-widget-option">
										<a for="blue">性格气质</a>
									</div>
									|
									<div class="sidebar-widget-option">
										<a for="brown">职业发展</a>
									</div>
								</div>
							</div>
							<h3>招聘真题</h3>
							<div class="sidebar-widget">
								<div class="sidebar-widget-option-wrapper">
									<div class="sidebar-widget-option">
										<a for="dior">互联网/软件</a>
									</div>
									|
									<div class="sidebar-widget-option">
										<a for="ferragamo">通信/电子</a>
									</div>
								</div>
							</div>
							<h3>学历教育</h3>
							<div class="sidebar-widget">
								<div class="sidebar-widget-option-wrapper">
									<div class="sidebar-widget-option">
										<a for="dior">小学</a>
									</div>
									|
									<div class="sidebar-widget-option">
										<a for="ferragamo">初中</a>
									</div>
									|
									<div class="sidebar-widget-option">
										<a for="ferragamo">高中</a>
									</div>
									|
									<div class="sidebar-widget-option">
										<a for="ferragamo">大学</a>
									</div>
									|
									<div class="sidebar-widget-option">
										<a for="ferragamo">研究生</a>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
