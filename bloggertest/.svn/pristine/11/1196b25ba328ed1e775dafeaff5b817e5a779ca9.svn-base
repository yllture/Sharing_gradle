		<script>
			$(function() {
                $("[data-toggle='tooltip']").tooltip(); 
                
				$('.single-product-item').hover(function() {
					$(this).find('.product-mask').show();
				},function(){
					$(this).find('.product-mask').hide();
				})
			})
		</script>
		<script type="text/javascript">
    function selectpagesize(obj){
        window.location.href="/tobankshop?pagesize="+obj;
    }
 $(document).ready(function (){
     if("${pagesize }"){                                   //重点是这个判断
         $("option[value='${pagesize }']").attr("selected",true);
     }else{
         $("option[value='3']").attr("selected",true);               
     }
 });
     function buybank(obj){ 
    	 
    	 window.location.href="/bankshopdetail?shop_bankid="+obj;
     }
     function sort(obj){
    	 var url="/tobankshop?sorttype="+obj;
    	 if($(session.chargetype))
    		 {
    		 url+="chargetype="+${session.chargetype}
    		 }
    	 
    	 window.location.href=url;
     }
</script>
