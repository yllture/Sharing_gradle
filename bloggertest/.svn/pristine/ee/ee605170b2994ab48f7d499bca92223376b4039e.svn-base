}
.folder-name span{
	margin-top: 10px;
    font-weight: normal;
    font-size: 13px;
    color: #FFF;
    background-color: #8CADCA;
    display: inline-block;
    vertical-align: middle;
    border-radius: 5px;
    padding: 2px 6px;
}
.custom-col {
    padding-left: 15px;
    padding-right: 15px;
}
.single-product-item {
    border: 1px solid #e7e7e7;
    border-radius: 20px;
    -webkit-border-radius: 20px;
    -moz-border-radius: 20px;
    box-shadow: 1px 1px 0 0 #ffffff inset, 1px 1px 0 0 #ffffff;
    margin: 0px 0 32px;
    overflow: hidden;
    width: 200px;
    background-color: #f1f1f1;
    padding: 10px 0 10px;
    -webkit-transition: all 0.3s ease-in-out 0s;
    -moz-transition: all 0.3s ease-in-out 0s;
    -ms-transition: all 0.3s ease-in-out 0s;
    -o-transition: all 0.3s ease-in-out 0s;
    transition: all 0.3s ease-in-out 0s;
    position: relative;
}
.single-product-item .product-text .pro-price {
    /*display: flex;*/
    font-family: "Oswald", sans-serif;
    font-size: 14px;
    justify-content: center;
}
.pro-info {
    margin: 30px 0px 15px 0px;
    padding: 0px 10px 0px 10px;
    border-top: solid 1px #EEEEEE;
}
.custom-col{
	    width: 230px;
	    display: table-cell;
}
.product-image{
	display: none;
}
.product-mask {
    position: absolute;
    height: 187px;
    width: 204px;
    margin-left: -3px;
    margin-top: -31px;
    border-radius: 5px;
    background-color: whitesmoke;
    opacity: 0.5;
}
.single-product-item .product-image .product-hover>ul.hover-icon-list {
    /*display: flex;*/
    justify-content: center;
    /*left: 0;*/
    /*opacity: 0;*/
    position: absolute;
    /*right: 0;*/
    top: 26%;
    left: 7px;
    list-style: none;
    -webkit-transition: all 0.3s ease-in-out 0s;
    -moz-transition: all 0.3s ease-in-out 0s;
    -ms-transition: all 0.3s ease-in-out 0s;
    -o-transition: all 0.3s ease-in-out 0s;
    transition: all 0.3s ease-in-out 0s;
}
.single-product-item .product-text>h5 {
    color: #202226;
